<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:dialog"
    jcr:title="Campaign"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <content
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./title"
                                value="Lorem Ipsum"/>
                            <description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Description"
                                name="./description"
                                value="Bibigo embodies the spirit of Korean cuisine, bringing families together through shared meals."/>
                        </items>
                    </content>
                    <form
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Form"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <nameLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Name Field Label"
                                name="./nameLabel"
                                value="Your Name"/>
                            <emailLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Email Field Label"
                                name="./emailLabel"
                                value="Email"/>
                            <phoneLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Phone Field Label"
                                name="./phoneLabel"
                                value="Phone"/>
                            <companyLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Company Field Label"
                                name="./companyLabel"
                                value="Company / Field of work"/>
                            <messageLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Message Field Label"
                                name="./messageLabel"
                                value="What are you looking for?"/>
                            <submitButtonText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Submit Button Text"
                                name="./submitButtonText"
                                value="keep in touch"/>
                        </items>
                    </form>
                    <media
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Media"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <backgroundImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                fieldLabel="Background Image"
                                fileNameParameter="./backgroundImageFileName"
                                fileReferenceParameter="./backgroundImageFileReference"
                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                multiple="{Boolean}false"
                                name="./backgroundImage"
                                title="Upload Background Image"/>
                            <logoImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                fieldLabel="Logo Image"
                                fileNameParameter="./logoImageFileName"
                                fileReferenceParameter="./logoImageFileReference"
                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                multiple="{Boolean}false"
                                name="./logoImage"
                                title="Upload Logo Image"/>
                        </items>
                    </media>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
